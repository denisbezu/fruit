  "require-dev": {
    "prestashop/php-dev-tools": "^3.4",
    "phpunit/phpunit": "^5",
    "phpstan/phpstan": "^0.12"
  },
  "autoload-dev": {
    "psr-4": {
      "Fruit\\": "src/"
